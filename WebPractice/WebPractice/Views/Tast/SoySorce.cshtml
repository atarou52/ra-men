@model WebPractice.Models.list
@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
    Layout = null;
}
    <html>
    <head>
        <meta name="viewport" content="width=device-width" />
        <title>醤油ラーメン</title>
    </head>
    <body>
        <h2>醤油ラーメン</h2>
        <hr />
    </body>
    </html>
    <th>
        <table border="1">
            <tr>
                <th>店の名前</th>
                <th>味の種類</th>
                <th>コメント</th>
                <th>点数</th>
            </tr>
        @if(Model.TastTexts is not null)
        {
            @foreach (var item in Model.TastTexts)
            {
                    <tr>
                        <td>@Html.DisplayFor( m => item.Name)</td>
                        <td>@Html.DisplayFor( m => item.Tast)</td>
                        <td>@Html.DisplayFor( m => item.Note)</td>
                        <td>@Html.DisplayFor( m => item.Score)</td>
                    </tr>
            }
        }
        </table>
    </th>

